<div class="containerz">
      <h1>PLAY TIC TAC TOE!</h1>
      <ul id ="board">
        <li position="0,0"></li>
        <li position="0,1"></li>
        <li position="0,2"></li>

        <li position="1,0"></li>
        <li position="1,1"></li>
        <li position="1,2"></li>

        <li position="2,0"></li>
        <li position="2,1"></li>
        <li position="2,2"></li>
      </ul>

      <button id="restart">Restart Game</button>

      <div id="alerts">
        <span class="px-win">Player 1 Wins!</span>
        <span class="po-win">Player 2 Wins!</span>
        <span class="draw">Another Round!?</span>
      </div>

      <aside id="info">
        <h2>Who's Turn</h2>
        <div id="player-turn" class="x">
          <span class="x">Player 1</span>
          <span class="o">Player 2</span>
        </div>

        <h2>Score</h2>
        <div id="score">
          P1: <span id="p1-score"></span> <button id="reset-1">Reset</button><br><br>
          P2: <span id="p2-score"></span> <button id="reset-2">Reset</button><br>
        </div>
      </aside>
    </div>
    <script src="/assets/application.js"></script>
